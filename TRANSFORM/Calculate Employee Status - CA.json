                },
                "type": "accountAttribute"
              },
              {
                "attributes": {
                  "attributeName": "BASE_ASSIGNMENT_STATUS_TYPE_ID",
                  "sourceName": "Oracle HCM Employees"
                },
                "type": "accountAttribute"
              }
            ]
          },
          "type": "firstValid"
        },
        "table": {
          "1": "active",
          "2": "active",
          "3": "inactive",
          "4": "inactive",
          "5": "suspended",
          "6": "suspended",
          "1001": "ltd",
          "1002": "ltd",
          "1003": "loa",
          "1004": "loa",
          "2001": "inactive",
          "4001": "leavePayrollEligible",
          "7001": "loa",
          "none": "active",
          "default": "invalid"
