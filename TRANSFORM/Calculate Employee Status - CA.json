        },
        "table": {
          "Origin & Cause Inc.": "OriginAndCauseInc",
          "none": "",
          "default": ""
        }
      },
      "type": "lookup"
    },
    "value": "#if(${employeeStatus} == 'active' && ${preHire} == 'True')prehire${countryCode}${sufix}#elseif(${employeeStatus} == 'active')${employeeStatus}${countryCode}${sufix}#{else}${employeeStatus}${countryCode}#end"
